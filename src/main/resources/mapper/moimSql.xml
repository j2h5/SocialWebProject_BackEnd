<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.moimMapper">
	 
	<insert id="insertmoim" parameterType="moim">
      insert into moim (moim_name,moim_leader,moim_place,moim_count,moim_photo,moim_content,moim_writeday)
      values (#{moim_name},#{moim_leader},#{moim_place},#{moim_count},#{moim_photo},#{moim_content},now())
   	</insert>
   
</mapper>